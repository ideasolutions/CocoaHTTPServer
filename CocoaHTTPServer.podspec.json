{
  "name": "CocoaHTTPServer",
  "version": "2.3.0.2",
  "license": "BSD",
  "summary": "A small, lightweight, embeddable HTTP server for Mac OS X or iOS applications.",
  "homepage": "https://github.com/ideasolutions/CocoaHTTPServer",
  "authors": {
    "Robbie Hanson": "cocoahttpserver@googlegroups.com"
  },
  "source": {
    "git": "https://github.com/ideasolutions/CocoaHTTPServer.git",
    "tag": "v2.3.0.2"
  },
  "source_files": "{Core,Extensions,Vendor/CocoaLumberjack}/**/*.{h,m}",
  "requires_arc": true,
  "platforms": {
    "ios": "5.0",
    "osx": "10.9"
  },
  "ios": {
    "frameworks": [
      "CFNetwork",
      "Security"
    ]
  },
  "osx": {
    "frameworks": [
      "CoreServices",
      "Security"
    ]
  },
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(SDKROOT)/usr/include/libxml2\""
  },
  "dependencies": {
    "CocoaAsyncSocket": [

    ]
  }
}
